r<vector<int>> fourSum(vector<int>& nums, int target) {\\n\\n        int size = nums.size();     // è·åæ°ç»é¿åº¦\\n        vector<vector<int>> result; // ç»æéå\\n        long tmpSum = 0;             // æ¯æ°ä¹å longé²æ­¢æº¢åº\\n        int left,right;             // åæé\\n\\n        // å°äº4å¤æ­\\n        if (size < 4) { return result; }\\n\\n        // æåº\\n        sort(nums.begin(), nums.end());\\n\\n        // å¾ªç¯a\\n        for(int a = 0; a < size - 3; ++a){\\n            \\n            // å¯¹aå»é\\n            if(a > 0 && nums[a] == nums[a-1]){continue;}\\n\\n            // å¾ªç¯b\\n            for(int b = a + 1; b < size - 2; ++b){\\n\\n                // å¯¹bå»é\\n                if(b > a + 1 && nums[b] == nums[b-1]){continue;}\\n\\n                // ä¸å±åªæ\\n                if(nums[a]+nums[b] > 0 && target > 0 && nums[a]+nums[b]>target){break;}\\n                // åæé\\n                left = b + 1;\\n                right = size - 1;\\n\\n                // åæéç§»å¨\\n                while(left < right){\\n\\n                    // æ±å\\n                    tmpSum = (long)nums[a]+nums[b]+nums[left]+nums[right];\\n\\n                    // å¯¹æ¯target\\n                    if(tmpSum > target){\\n                        --right;\\n                    }else if(tmpSum < target){\\n                        ++left;\\n                    }else{\\n                        result.push_back(vector<int>{nums[a],nums[b],nums[left],nums[right]});\\n\\n                        // left å»é\\n                        while(left < right && nums[left] == nums[left+1]){++left;}\\n\\n                        // rightå»é\\n                        while(left < right && nums[right] == nums[right-1]){--right;}\\n\\n                        ++left;\\n                        --right;\\n                    }\\n                }\\n            }\\n        }\\n\\n        return result;\\n    }\\n};\\n\\n\\n1\\n2\\n3\\n4